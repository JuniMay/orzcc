// double loop
#include <stdio.h>
int main(){
    const int n = 5;
    const int m = 11; // Need: m >= 2n+1

    int loop = n;
    int x[n] = {};
    int y[n]  = {};
    int i = 1;
    while (i<=n){
        y[i-1] = i;
        i = i+1;
    }
    
    int z[m]  = {};
    i = m;
    int j = 0;
    while (i>0){
        z[j] = i;
        i = i-1;
        j = j+1;
    }

    int q = 1, r = 2, t = 3;

    int l = 1;
    while (l <= loop) {
        int k = 0;
        while (k < n) {
            x[k] = q + y[k] * (r * z[k + 1] + t * z[k + 2]);
            k = k+1;
        }
        l = l+1;
    }

    printf("%d", x[0]);
    return 0;
}